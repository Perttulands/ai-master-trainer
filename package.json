  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@reactflow/background": "^11.3.14",
    "@reactflow/controls": "^11.2.14",
    "@reactflow/core": "^11.11.4",
    "dagre": "^0.8.5",
    "file-saver": "^2.0.5",
    "lucide-react": "^0.468.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.1.0",
    "reactflow": "^11.11.4",
    "sql.js": "^1.11.0",
    "uuid": "^11.0.0",
    "zustand": "^5.0.0"
